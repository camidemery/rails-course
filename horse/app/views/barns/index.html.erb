<%= link_to 'New', new_barn_path %>

<%=@barn.each do |barn|%>
<div>
    <h2><%=barn.name%></h2>
    <p><%=barn.owner%></p>
    <p><%=barn.location%></p>

    <%= link_to "Edit", edit_barn_path(barn.id)%>
    <%= link_to 'Show', barn_path(barn)%>
    <%= link_to "Delete", barn_path(barn.id), method: :delete%> 
</div>
<%end%>